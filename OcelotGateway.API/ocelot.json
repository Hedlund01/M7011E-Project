{
  "Routes": [
    {
      "DownstreamPathTemplate": "/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "identity.api",
          "Port": 8080
        }
      ],
      "UpstreamPathTemplate": "/api/{version}/identity/{everything}",
      "UpstreamHttpMethod": [
        "POST",
        "PUT",
        "GET"
      ],
      "SwaggerKey": "Identity"
    },
    {
      "DownstreamPathTemplate": "/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "catalog.api",
          "Port": 8080
        }
      ],
      "UpstreamPathTemplate": "/api/{version}/catalog/{everything}",
      "UpstreamHttpMethod": [
        "POST",
        "PUT",
        "GET"
      ],
      "SwaggerKey": "Catalog"
    }
  ],
  "GlobalConfiguration": {
    "RequestIdKey": "OcRequestId",
    "AdministrationPath": "/administration"
  },
  "SwaggerEndPoints": [
    {
      "Key": "Identity",
      "Config": [
        {
          "Version": "v1",
          "Name": "Identity API",
          "Url": "http://identity.api:8080/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "Catalog",
      "Config": [
        {
          "Version": "v1",
          "Name": "Catalog API",
          "Url": "http://catalog.api:8080/swagger/v1/swagger.json"
        }
      ]
    }
  ]
}